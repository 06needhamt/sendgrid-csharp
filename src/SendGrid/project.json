{
	"buildOptions": {
		"emitEntryPoint": false,
		"keyFile": "../../tools/sendgrid-csharp.snk",
		"xmlDoc": true,
		"platform": "anycpu",
		"compile": {
		}
	},
	"configurations": {
		"Release": {
			"buildOptions": {
				"warningsAsErrors": false
			}
		}
	},
  "dependencies": {
    "Microsoft.AspNetCore.Http.Abstractions": "1.1.0"
  },
	"frameworks": {
		"netstandard1.3": {
			"buildOptions": {
				"define": [ "NETSTANDARD" ]
			},
			"imports": "dnxcore50",
			"dependencies": {
				"NETStandard.Library": "1.6.1",
				"Newtonsoft.Json": "9.0.1"
			}
		},
		"net452": {
			"buildOptions": {
				"define": [ "NET452" ]
			},
			"dependencies": {
				"Newtonsoft.Json": "9.0.1",
				"System.Net.Http": "4.0.0"
			},
			"frameworkAssemblies": {
				"System.Runtime.Serialization": "4.0.0"
			}
		}
	},
	"version": "9.0.5-beta"
}
